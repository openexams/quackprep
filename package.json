{
  "name": "quackprep",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm install; npm run start:backend",
    "dev": "concurrently --kill-others-on-fail --success=first \"npm run start:backend\" \"npm run dev:frontend\"",
    "build:frontend": "cd frontend && npm i && npm run build",
    "start:backend": "cd backend && npm i && npm start",
    "dev:frontend": "cd frontend && npm i && npm run dev"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "concurrently": "^9.0.1",
    "eslint": "^9.16.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.13.0",
    "install": "^0.13.0",
    "npm": "^10.9.2",
    "playwright": "^1.49.0"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "form-data": "^4.0.1",
    "rand-seed": "^2.1.7"
  }
}
